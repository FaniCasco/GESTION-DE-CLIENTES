{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "Sistema de gestion de clientes INMOBILIARIA",
  "version": "0.1.0",
  "identifier": "com.tauri.inmobiliaria",
  "build": {
    "frontendDist": "../frontend/build",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm start",
     "beforeBuildCommand": "cd frontend && npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "INMOBILIARIA",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
     "csp": null,
  "devCsp": "default-src * data: blob: 'unsafe-inline' 'unsafe-eval';"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}

