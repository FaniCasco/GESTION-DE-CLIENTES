  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },

  "build": {
    "appId": "com.FaniCas.frontend",  
    "productName": "Sistema de gesti√≥n de clientes",  
    "files": [
      "build/**/*",
      "src/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis", 
      "icon": "build/icon.ico"  
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": "AppImage",
