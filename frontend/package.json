    "axios": "^1.7.9",
    "bootstrap-icons": "^1.11.3",
    "cra-template": "1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-modal": "^3.16.3",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.28.1",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",    
    "build": "react-scripts build",
    "test": "react-scripts test",
